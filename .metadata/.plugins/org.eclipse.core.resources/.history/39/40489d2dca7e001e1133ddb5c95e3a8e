
public class Restaurant{
	boolean servePizza(CanHavePizza eater) {
		if(eater instanceof Person) {
			System.out.println("Processing payments for the person.");
			Person person =(Person)eater;
			person.payment();
        }
		else if{
			Cat cat = (Cat)eater;
			cat.eatPizza();		}
        return true;
	}
}
