
public class Rectangle extends Shape{
	private int width;
	private int height;

	public Rectangle(int position, Color color, int width, int height) {
		super(position, color);
	    this.width = width;
	    this.height = height;
	 }

	public void draw() {
		System.out.println("shape: rectangle "+", color: "+color+", width: "+width+", height: "+height+", position "+position);
	}
}
